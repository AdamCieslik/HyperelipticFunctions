	file = {Preprint PDF:/Users/adamcieslik/Zotero/storage/5GR5Z72D/Eilbeck et al. - 2017 - Theory of Heat Equations for Sigma Functions.pdf:application/pdf;Snapshot:/Users/adamcieslik/Zotero/storage/TRXAHTWE/1711.html:text/html},
