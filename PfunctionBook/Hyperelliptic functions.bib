@misc{buchstaber_multi-dimensional_2012,
	title = {Multi-{Dimensional} {Sigma}-{Functions}},
	url = {http://arxiv.org/abs/1208.0990},
	abstract = {In 1997 the present authors published a review (Ref. BEL97 in the present manuscript) that recapitulated and developed classical theory of Abelian functions realized in terms of multi-dimensional sigma-functions. This approach originated by K.Weierstrass and F.Klein was aimed to extend to higher genera Weierstrass theory of elliptic functions based on the Weierstrass \${\textbackslash}sigma\$-functions. Our development was motivated by the recent achievements of mathematical physics and theory of integrable systems that were based of the results of classical theory of multi-dimensional theta functions. Both theta and sigma-functions are integer and quasi-periodic functions, but worth to remark the fundamental difference between them. While theta-function are defined in the terms of the Riemann period matrix, the sigma-function can be constructed by coefficients of polynomial defining the curve. Note that the relation between periods and coefficients of polynomials defining the curve is transcendental. Since the publication of our 1997-review a lot of new results in this area appeared (see below the list of Recent References), that promoted us to submit this draft to ArXiv without waiting publication a well-prepared book. We complemented the review by the list of articles that were published after 1997 year to develop the theory of \${\textbackslash}sigma\$-functions presented here. Although the main body of this review is devoted to hyperelliptic functions the method can be extended to an arbitrary algebraic curve and new material that we added in the cases when the opposite is not stated does not suppose hyperellipticity of the curve considered.},
	language = {en},
	urldate = {2024-10-28},
	publisher = {arXiv},
	author = {Buchstaber, V. M. and Enolski, V. Z. and Leykin, D. V.},
	month = aug,
	year = {2012},
	note = {arXiv:1208.0990 [math-ph]},
	keywords = {Mathematical Physics, Nonlinear Sciences - Exactly Solvable and Integrable Systems, Mathematics - Mathematical Physics},
	annote = {Comment: 267 pages, 4 figures},
	file = {Buchstaber et al. - 2012 - Multi-Dimensional Sigma-Functions.pdf:/Users/adamcieslik/Zotero/storage/WDYF2TEI/Buchstaber et al. - 2012 - Multi-Dimensional Sigma-Functions.pdf:application/pdf},
}
